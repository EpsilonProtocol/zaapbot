{
	"name": "zaapbot",
	"version": "0.0.0",
	"private": true,
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"scripts": {
		"build": "turbo run build",
		"clean": "turbo run clean",
		"dev": "dotenv -- turbo run dev",
		"format": "prettier --write \"**/*.{ts,tsx,md}\"",
		"lint": "turbo run lint",
		"test": "turbo run test"
	},
	"devDependencies": {
		"dotenv-cli": "latest",
		"eslint-config-custom": "*",
		"prettier": "^2.8.8",
		"turbo": "1.10.4"
	},
	"packageManager": "yarn@1.22.1",
	"engines": {
		"node": ">=18.0.0",
		"npm": ">=7.0.0"
	}
}
